
.music-head {
	position: relative;
	width: 100%;
	height: 75px;
	font-size: 16px;
	line-height: 80px;
	text-align: center;
	color: #000;
}
.music-head-icon {
	display: flex;
	justify-content: space-evenly;
	position: absolute;
	left: 8px;
	top: 26px;
	width: 90px;
	height: 31px;
	line-height: 31px;
	background: rgba(0, 0, 0, 0.5);
	color: #fff;
	border-radius: 15px;
}
.music-head-iconblack {
	color: #000;
	border: 1px solid #eaeaea;
	background: #fff;
}

